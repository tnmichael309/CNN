layers {
  name: "concat_all_channels"
  bottom: "channel_1"
  bottom: "channel_2"
  bottom: "channel_3"
  bottom: "channel_4"
  bottom: "channel_5"
  bottom: "channel_6"
  bottom: "channel_7"
  top: "concat_all_channels"
  type: CONCAT
  concat_param {
    concat_dim: 1
  }
}

layers {
  name: "conv1"
  type: CONVOLUTION
  bottom: "concat_all_channels"
  top: "conv1"
  blobs_lr: 1
  blobs_lr: 2
  convolution_param {
    num_output: 128
    kernel_size: 5
    stride: 1
	pad: 4
    weight_filler {
      type: "gaussian"
      std: 0.0001
    }
    bias_filler {
      type: "constant"
	  value: 0
    }
  }
}

